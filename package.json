{
  "name": "@celiumgrid/ts-no-duplicate",
  "private": false,
  "version": "0.0.2",
  "description": "TypeScript cross-file duplicate name detection tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "tsnd": "dist/index.js",
    "ts-no-duplicate": "dist/index.js"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "tsdown",
    "dev": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "commit": "git-cz",
    "prepare": "husky",
    "release": "release-it"
  },
  "keywords": [
    "typescript",
    "ast",
    "duplicate",
    "detection"
  ],
  "author": "cola",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "jiti": "^2.6.1",
    "lodash-es": "^4.17.21",
    "minimatch": "^10.0.1",
    "ts-morph": "^27.0.2",
    "tsdown": "^0.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@release-it/conventional-changelog": "^10.0.3",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/glob": "^9.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.2",
    "git-cz": "^4.9.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "release-it": "^19.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
